<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.todolist.GetIdWindow"
            prefHeight="50.0" prefWidth="280.0">
    <children>
        <Button layoutX="206.0" layoutY="14.0" onAction="#openChangeWindow" mnemonicParsing="false" text="Ok" />
        <Label contentDisplay="RIGHT" layoutX="14.0" layoutY="14.0" text="Id">
            <graphic>
                <TextField fx:id="getIDChange"/>
            </graphic>
        </Label>
    </children>
</AnchorPane>
